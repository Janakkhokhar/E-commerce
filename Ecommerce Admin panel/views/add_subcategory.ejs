<%-include('header')%>

    <main id="main" class="main">

        <div class="pagetitle">
            <h1>Add SubCategory</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                    <li class="breadcrumb-item">Forms</li>
                    <li class="breadcrumb-item active">subcategory</li>
                </ol>
            </nav>
        </div><!-- End Page Title -->

        <section class="section">
            <div class="row">
                <div class="col-lg-12">

                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">Add Form </h5>

                            <!-- General Form Elements -->
                            <form method="post" action="/admin/subcate/insertsubcateData">
                               
                                <div class="row mb-3">
                                    <label for="select" class="col-sm-2 col-form-label">Title Category</label>
                                    <div class="col-sm-10">
                                        <select name="category_name" id="select" class="form-control">
                                            <option value="">-- Title Category -- </option>
                                            <%for(var sd of catRecord){%>

                                                <option value="<%=sd.id%>"><%=sd.category_name%></option>
                                            <%}%>   
                                        </select>
                                    </div>
                                </div>

                                <div class="row mb-3">
                                    <label for="select" class="col-sm-2 col-form-label">SubCate Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" name="subcate_name">
                                    </div>
                                </div>


                                <div class="row mb-3">
                                    <div class="col-sm-10">
                                        <button type="submit" class="btn btn-primary btn-sm"> Submit
                                        </button>
                                        <button type="reset" class="btn btn-danger btn-sm"> Reset
                                        </button>
                                    </div>
                                </div>

                            </form><!-- End General Form Elements -->

                        </div>
                    </div>

                </div>


            </div>
        </section>

    </main><!-- End #main -->

    <%-include('footer')%>