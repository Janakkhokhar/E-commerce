<script>
  function getsubcategoryData() {
    var catId = document.getElementById('categoryId').value;
    // alert(catId);
    $.ajax({
      url: "/admin/extra/getsubcatData",
      method: "post",
      data: {
        categoryId: catId
      },
      success: function (res) {
        $("#subcategoryId").html(res);
      }
    })
  }



  function getextracategoryData() {
    var subcatId = document.getElementById('subcategoryId').value;
    // alert(subcatId);
    $.ajax({
      url: "/admin/brand/getextracatData",
      method: "post",
      data: {
        subcategoryId: subcatId
      },
      success: function (res) {
        $("#extracategoryId").html(res);
      }
    })
  }



  function getbrandcategoryData() {
    var subcategoryId = document.getElementById('subcategoryId').value;
    var extracatId = document.getElementById('extracategoryId').value;
    var categoryId = document.getElementById('categoryId').value;

    $.ajax({
      url: "/admin/type/gettypecatData",
      method: "post",
      data: {
        extracategoryId: extracatId,
        categoryId: categoryId,
        subcategoryId: subcategoryId
      },
      success: function (res) {
        $("#brandType").html(res);
      }
    })
  }








</script>